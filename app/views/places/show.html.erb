<h1><%= @place["name"] %></h1>

<p><a href="/places">Back to all places</a></p>

<ul>
  <% for entry in @entries %>
    <li>
      <strong><%= entry["title"] %></strong> - <%= entry["occurred_on"] %>
      <p><%= entry["description"] %></p>
    </li>
  <% end %>
</ul>

<p><a href="/entries/new?place_id=<%= @place["id"] %>">Log a new entry</a></p>