<h1><%= @place["name"] %></h1>

<p><a href="/places">Back to Places I've Been</a></p>

<ul>
  <% for entry in @entries %>
    <li>
      <strong><%= entry["title"] %></strong>
      <p>Date: <%= entry["occurred_on"] %></p>
      <p>Decription: <%= entry["description"] %></p>
    </li>
  <% end %>
</ul>

<p><a href="/entries/new?place_id=<%= @place["id"] %>">Log a new entry for <%= @place["name"] %></a></p>